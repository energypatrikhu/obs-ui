{
  "name": "@obs-ui/extension",
  "description": "Energy's OBS Now Playing Extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build && bun ./move.ts",
    "build:firefox": "wxt build -b firefox && bun ./move.ts",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "wxt prepare"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.5",
    "@types/lodash": "^4.17.20",
    "@wxt-dev/module-svelte": "^2.0.3",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  },
  "dependencies": {
    "@energypatrikhu/node-utils": "^0.2.3",
    "lodash": "^4.17.21"
  }
}
