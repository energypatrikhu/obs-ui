{
  "name": "@obs-ui/extension",
  "description": "Energy's OBS Now Playing Extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build && bun ./move.ts",
    "build:firefox": "wxt build -b firefox && bun ./move.ts",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepare": "wxt prepare"
  },
  "devDependencies": {
    "@tsconfig/svelte": "^5.0.7",
    "@types/lodash": "4.17.23",
    "@wxt-dev/module-svelte": "2.0.4",
    "svelte": "^5.50.0",
    "svelte-check": "^4.3.6",
    "tslib": "2.8.1",
    "typescript": "5.9.3",
    "wxt": "^0.20.14"
  },
  "dependencies": {
    "@energypatrikhu/node-utils": "0.2.3",
    "lodash": "4.17.23"
  }
}
